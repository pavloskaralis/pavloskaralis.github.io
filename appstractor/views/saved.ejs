<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <!-- <link rel="stylesheet" href="/../css/canvas.css">  -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <style>
        html, body, #overlay, .top-canvas, .bottom-canvas, .background-canvas {
        width: 3600px;
        height: 2400px; 
        margin: 0;
        position: absolute;
        }

        body {
        background-color:rgb(10,15,20);
        }

        .top-canvas {
        display: flex; 
        flex-wrap: wrap-reverse;
        flex-direction: row;
        z-index: 5;
        justify-content: flex-end; 
        }

        .bottom-canvas { 
        z-index: 4;
        display: flex; 
        flex-wrap: wrap;
        justify-content: flex-start;
        }

        .background-canvas {
        z-index: 3; 
        background-position: center;
        background-size: cover; 
        }

        .block {
        display: flex;  
        }
    </style>
    <title>Saved</title>
</head>
<div id="temp">
        <%= data %>
</div>
<script>
    const $parse = $( '<body>' );
    $parse.html($('#temp').text());
    $('body').remove();
    $('html').append($parse);
    //remove <script> 
    // $parse.children().eq(3).remove();
    $('#download',parent.document).on('click', () => {
      
        function downloadInnerHtml(filename, elId, mimeType) {
        const elHtml = $(elId).html();
        const link = document.createElement('a');
        mimeType = mimeType || 'text/plain';
            $(link).attr('download', filename);
            $(link).attr('href', 'data:' + mimeType + ';charset=utf-8,' + encodeURIComponent(elHtml));
            link.click(); 
        }
        const fileName =  'appstractor.html'; // You can use the .txt extension if you want
        downloadInnerHtml(fileName, 'html','text/html');

    });
</script>
</html>